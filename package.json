{
  "name": "app-seed",
  "version": "0.0.1",
  "description": "app-seed is an application skeleton for web apps. It uses AngularJS for front-end and hapi for back-end. It is suitable for Single Page Application (SPA) and provides user authentication with Google OAuth 2.0",
  "keywords": [
    "web",
    "app",
    "angularjs",
    "hapi",
    "google",
    "oauth2",
    "spa",
    "seed"
  ],
  "homepage": "http://github.com/cmfatih/app-seed",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmfatih/app-seed.git"
  },
  "bugs": {
    "url": "http://github.com/cmfatih/app-seed/issues"
  },
  "license": "MIT",
  "private": false,
  "author": {
    "name": "cmfatih",
    "url": "http://github.com/cmfatih"
  },
  "contributors": [],
  "scripts": {
    "start": "node ./services/httpd/index.js -c config/app.json",
    "test": "node ./services/httpd/index.js -c config/test.json"
  },
  "engines": [
    "node >= 1.10.21"
  ],
  "dependencies": {
    "hapi": "1.x",
    "yar": "0.4.x",
    "googleapis": "0.4.x"
  }
}