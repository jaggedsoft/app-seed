{
  "name": "app-seed",
  "version": "0.2.0",
  "description": "app-seed is an application skeleton for web apps. It uses AngularJS for front-end and Node.js (with hapi) for back-end. It is suitable for Single Page Application (SPA) and provides user authentication with Google OAuth 2.0",
  "keywords": [
    "web",
    "app",
    "angularjs",
    "hapi",
    "google",
    "oauth2",
    "spa",
    "seed",
    "html5"
  ],
  "homepage": "http://github.com/cmfatih/app-seed",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmfatih/app-seed.git"
  },
  "bugs": {
    "url": "http://github.com/cmfatih/app-seed/issues"
  },
  "license": "MIT",
  "private": false,
  "author": {
    "name": "cmfatih",
    "url": "http://github.com/cmfatih"
  },
  "contributors": [],
  "scripts": {
    "start": "node ./app/httpd/index.js -c config/app.json",
    "test": "node ./app/httpd/index.js -c config/test.json"
  },
  "engines": [
    "node >= 0.10.21"
  ],
  "dependencies": {
    "utilex": "0.2.x",
    "hapi": "2.x",
    "yar": "1.x",
    "googleapis": "0.6.x"
  }
}